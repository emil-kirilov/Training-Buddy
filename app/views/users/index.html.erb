<table>
	<tr>
		<th>id</th>
		<th>email</th>
		<th>craeted at</th>
	</tr>

	<tbody>
    <% if @users.empty? %>
      <strong> No users yet</strong>
    <% else %>
    	<% @users.each do |user| %>
    		<tr>
    			<td><%= user.id %></td>
    			<td><%= user.email %></td>
    			<td><%= user.created_at %></td>
          <td></td>
    		</tr>
    	<% end %>
		<% end %>
	</tbody>
</table>